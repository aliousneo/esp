<thead>
    <tr>
        <th>Product key</th>
        <th>Description</th>
        <th>Score</th>
    </tr>
</thead>
<tbody>
    {% for result in search_results %}
    <tr>
        <td>{{ result.product_key }}</td>
        <td>{{ result.description }}</td>
        <td>{{ result.score }}</td>
    </tr>
    {% endfor %}
</tbody>